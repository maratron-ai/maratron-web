// @utils/math/lerp

// linear estimator, takes in start, end, and  and returns the interpolated value

// defined as
// start(t=start), end(t=end), t=[start/n,end/n] 

const lerp = (start, end, t) => {
  /* istanbul ignore next */
  cov_1blhsnwbc6().f[0]++;
  cov_1blhsnwbc6().s[1]++;
  return start + (end - start) * t;
};
