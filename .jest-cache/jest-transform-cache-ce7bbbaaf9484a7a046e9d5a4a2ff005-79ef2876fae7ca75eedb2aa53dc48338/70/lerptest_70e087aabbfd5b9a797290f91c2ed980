"use strict";

var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _lerp = _interopRequireDefault(require("../math/lerp"));
describe("lerp", () => {
  it("interpolates between start and end", () => {
    expect((0, _lerp.default)(0, 10, 0)).toBe(0);
    expect((0, _lerp.default)(0, 10, 1)).toBe(10);
    expect((0, _lerp.default)(0, 10, 0.5)).toBe(5);
  });
});
