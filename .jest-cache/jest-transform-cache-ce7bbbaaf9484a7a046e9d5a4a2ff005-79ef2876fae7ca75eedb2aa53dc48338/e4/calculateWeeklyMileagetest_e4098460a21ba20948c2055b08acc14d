"use strict";

var _calculateWeeklyMileage = require("../running/calculateWeeklyMileage");
describe("calculateWeeklyMileage", () => {
  it("handles each training phase", () => {
    const totalWeeks = 10;
    const peak = 50;
    expect((0, _calculateWeeklyMileage.calculateWeeklyMileage)(1, totalWeeks, peak)).toBe(28); // base
    expect((0, _calculateWeeklyMileage.calculateWeeklyMileage)(5, totalWeeks, peak)).toBe(38); // build
    expect((0, _calculateWeeklyMileage.calculateWeeklyMileage)(8, totalWeeks, peak)).toBe(50); // peak
    expect((0, _calculateWeeklyMileage.calculateWeeklyMileage)(10, totalWeeks, peak)).toBe(25); // taper
  });
});
