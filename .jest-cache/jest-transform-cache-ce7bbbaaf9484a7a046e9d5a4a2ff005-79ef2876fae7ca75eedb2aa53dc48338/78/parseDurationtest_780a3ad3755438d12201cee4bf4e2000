"use strict";

var _parseDuration = require("../time/parseDuration");
describe("parseDuration", () => {
  it("converts hh:mm:ss to seconds", () => {
    expect((0, _parseDuration.parseDuration)("01:00:00")).toBe(3600);
    expect((0, _parseDuration.parseDuration)("00:30:15")).toBe(1815);
  });
  it("converts mm:ss to seconds", () => {
    expect((0, _parseDuration.parseDuration)("10:30")).toBe(630);
  });
});
