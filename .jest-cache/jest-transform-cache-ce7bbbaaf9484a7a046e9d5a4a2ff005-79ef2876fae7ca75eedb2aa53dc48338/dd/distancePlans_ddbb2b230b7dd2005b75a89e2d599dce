function toDistance(unit, milesVal, kmVal) {
  return unit === "kilometers" ? kmVal : milesVal;
}
function generate5kPlan(options) {
  const {
    weeks = 8,
    distanceUnit,
    trainingLevel,
    vdot
  } = options;
  const dist = toDistance(distanceUnit, 3.1, 5);
