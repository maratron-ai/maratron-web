"use strict";

var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault");
var _calculatePace = _interopRequireDefault(require("../running/calculatePace"));
describe("calculatePace", () => {
  it("computes pace from duration and distance", () => {
    expect((0, _calculatePace.default)("01:00:00", 10)).toBe("06:00");
    expect((0, _calculatePace.default)("00:30:00", 5)).toBe("06:00");
  });
});
