"use strict";

var _paces = require("../running/paces");
describe("pace utilities", () => {
  it("parses and formats pace", () => {
    expect((0, _paces.parsePace)("5:30")).toBe(330);
    expect((0, _paces.formatPace)(330)).toBe("5:30");
  });
  it("calculates training paces from race pace", () => {
    const paces = (0, _paces.getPacesFromRacePace)(330);
    expect(paces).toEqual({
      easy: "7:00",
      marathon: "5:45",
      threshold: "5:15",
      interval: "5:00",
      race: "5:30"
    });
  });
});
