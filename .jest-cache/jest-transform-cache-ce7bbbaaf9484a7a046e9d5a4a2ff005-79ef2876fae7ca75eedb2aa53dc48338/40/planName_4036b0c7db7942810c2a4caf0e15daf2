exports.defaultPlanName = defaultPlanName;
exports.getDistanceLabel = getDistanceLabel;
const LABELS = {
  "5k": "5K",
  "10k": "10K",
  half: "Half Marathon",
  full: "Marathon"
};
function getDistanceLabel(race) {
  return LABELS[race] || "";
}
function defaultPlanName(race, count) {
  const label = getDistanceLabel(race);
  return `${label} Training Plan ${count}`;
}
